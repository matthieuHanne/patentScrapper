<script type="text/javascript">
    function toggleFirst() {
        var $candidates = $('.iconPatentList,.iconPatentListSelected');
        var $nottoggled = $candidates.not('.toggled').first();
        if (!$nottoggled.length) {
            // All elements are toggled
            // Remove class from all links
            $candidates.removeClass('toggled');
            $nottoggled = $candidates.first();
        }
        // Add temporary class, trigger click
        $nottoggled.addClass('toggled').click();
    }

    // Function to handle ctrl+*
    function toggleAll() {
        $(".iconPatentList").click();
        // Add temporary class
        $(".iconPatentListSelected").addClass('toggled');
    }

    $(document).ready(function() {
        // Handle Ctrl+* and *
        $(document).keypress(function(e) { // CRUCIAL to use keypressed
            var $this = $(this);
            if (e.which === 42) { // '*'.charCodeAt(0) === 42
                if (!$this.data('star_pressed')) {
                    $this.data('star_pressed', true);
                    if (e.ctrlKey) { // Ctrl + *
                        toggleAll();
                    } else { // Just *
                        toggleFirst();
                    }
                }
            }
        }).keyup(function() {
                    $(this).removeData('star_pressed');
                });
    });
</script>



